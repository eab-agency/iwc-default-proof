{
  "name": "iwcproofer",
  "private": false,
  "description": "A simplified bare-bones starter for proofing IWCs",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "env-cmd -f .env.development gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@ux_bob/yv-iwc": "^1.1.10",
    "babel-plugin-prismjs": "^2.0.1",
    "babel-preset-gatsby": "^1.12.0",
    "gatsby": "^3.12.1",
    "gatsby-plugin-create-client-paths": "^3.12.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-react-helmet": "^4.12.0",
    "gatsby-plugin-sass": "^4.12.0",
    "gatsby-source-filesystem": "^3.12.0",
    "gatsby-source-graphql": "^3.12.0",
    "node-sass": "^6.0.1",
    "prismjs": "^1.23.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "scriptjs": "^2.5.9",
    "use-page-headings-tree": "^1.0.8"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "pre-push": "^0.1.1",
    "prettier": "^2.3.2"
  },
  "pre-push": [
    "build"
  ],
  "browser": {
    "crypto": false
}
}
